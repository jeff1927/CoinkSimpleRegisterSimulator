<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myapps.coinksimpleregistersimulator">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".CoinkSimpleRegisterSimulatorApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="CoinkRegisterSimulator"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoinkSimpleRegisterSimulator">
        <activity
            android:name=".ui.SplashActivity"
            android:theme="@style/Theme.App.Starting"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.MainActivity"
            android:exported="true">
        </activity>
        <meta-data
            android:name="keyValue"
            android:value="${PUBLIC_API_KEY}" />
    </application>

</manifest>